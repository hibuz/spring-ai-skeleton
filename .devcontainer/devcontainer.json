{
    "name": "Java",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/spring-ai",
    "postCreateCommand": "java -version",
    "features": {
      "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "mhutchie.git-graph",
          "seatonjiang.gitmoji-vscode"
        ]
      }
    },
    "forwardPorts": [
        8080,
        11434
    ],
    "remoteUser": "vscode"    
}